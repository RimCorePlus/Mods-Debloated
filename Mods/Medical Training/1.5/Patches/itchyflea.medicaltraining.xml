<Patch>
    <Operation Class="VFECore.PatchOperationToggableSequence">
        <enabled>true</enabled>
        <label>[Medical Training] Remove (tm) from table name</label>
        <operations>
            <li Class="PatchOperationReplace">
                <xpath>//ThingDef[defName="IF_MeditrainTable"]/label/text()</xpath>
                <value>meditrain table</value>
            </li>
        </operations>
    </Operation>
    <Operation Class="VFECore.PatchOperationToggableSequence">
        <enabled>true</enabled>
        <label>[Medical Training] Remove wood from costlist</label>
        <operations>
            <li Class="PatchOperationRemove">
                <xpath>//ThingDef[defName="IF_MeditrainTable"]/costList/WoodLog</xpath>
            </li>
        </operations>
    </Operation>
</Patch>