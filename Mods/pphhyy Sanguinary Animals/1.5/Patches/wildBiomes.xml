<Patch>
    <Operation Class="VFECore.PatchOperationToggableSequence">
        <enabled>True</enabled>
        <label>[pphhyy Sanguinary Animals] prevent wild animals from spawning</label>
        <operations>
            <li Class="PatchOperationRemove">
                <xpath>//ThingDef[@ParentName="AnimalThingBase" or @ParentName="BaseInsect" and starts-with(defName, "pphhyy_")]/race/wildBiomes</xpath>
            </li>
        </operations>
    </Operation>
</Patch>