<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>true</enabled>
		<label>[Combat Extended] Remove duplicate gas mask</label>
		<operations>
			<li Class="PatchOperationRemove">
				<xpath>//ThingDef[defName="CE_Apparel_GasMask"]</xpath>
			</li>
		</operations>
	</Operation>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>true</enabled>
		<label>[Combat Extended] Remove exoframe from pawn generation</label>
		<operations>
			<li Class="PatchOperationRemove">
				<xpath>//ThingDef[defName="CE_Apparel_ExoFrame"]/apparel/defaultOutfitTags | //ThingDef[defName="CE_Apparel_ExoFrame"]/apparel/tags</xpath>
			</li>
		</operations>
	</Operation>
</Patch>