<Patch>
    <!--not toggable-->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>RimJobWorld</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>//ThingDef[defName="EndTable"]</xpath>
                    <value>
                        <thingClass>AdaptiveStorage.ThingClass</thingClass>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>//ThingDef[defName="EndTable"]</xpath>
                    <value>
                        <drawGUIOverlay>True</drawGUIOverlay>
                        <inspectorTabs>
                            <li>ITab_Storage</li>
                        </inspectorTabs>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>//ThingDef[defName="EndTable"]/graphicData</xpath>
                    <value>
                        <shaderType>CutoutComplex</shaderType>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>//ThingDef[defName="EndTable"]</xpath>
                    <value>
                        <building>
                            <blueprintClass>Blueprint_Storage</blueprintClass>
                            <maxItemsInCell>1</maxItemsInCell>
                            <storageGroupTag>Shelf</storageGroupTag>
                            <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
                            <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
                            <fixedStorageSettings>
                                <filter>
                                    <disallowNotEverStorable>true</disallowNotEverStorable>
                                    <thingDefs>
                                        <li>Condom</li>
                                    </thingDefs>
                                </filter>
                            </fixedStorageSettings>
                            <defaultStorageSettings>
                                <priority>Preferred</priority>
                                <filter>
                                    <thingDefs>
                                        <li>Condom</li>
                                    </thingDefs>
                                </filter>
                            </defaultStorageSettings>
                        </building>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>