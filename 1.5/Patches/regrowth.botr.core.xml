<Patch>
    <Operation Class="VFECore.PatchOperationToggableSequence">
        <enabled>True</enabled>
        <label>[ReGrowth: Core] remove grassland</label>
        <operations>
            <li Class="PatchOperationFindMod">
                <mods>
                    <li>ReGrowth: Core</li>
                </mods>
                <match Class="PatchOperationSequence">
                    <operations>
                        <li Class="PatchOperationRemove">
                            <xpath>//BiomeDef[defName="RG_TemperateGrassland"] | //li[text()="RG_TemperateGrassland"] | //RG_TemperateGrassland</xpath>
                        </li>
                    </operations>
                </match>
            </li>
        </operations>
    </Operation>
</Patch>